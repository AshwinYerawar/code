a="Soham"
print(a)
print(a[0])
print(a[-1])  #index
print(a[1:3]) #slicing
s="Soham "
b="Sangare"
print("Conctination:- ",s+b)
print(s*4)
#escape sequance
print('''He said,"What is there"''')
print('He said,"What is there"')
print("He said,\"What is there\"")
w="i am Soham SANgare"
print(w.capitalize())
print(a.center(50))
print(len(a))
print(w.count("a"))
print(w.replace("i am","My nam is"))
print(w.split(" "))
print(a.upper())
print(a.lower())
print(a.endswith("m"))
